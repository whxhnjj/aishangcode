/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.wrap{
  width: 700rpx;
  margin: 0 25rpx;
}
/*灰色颜色占位符*/
.gray{
  height:50rpx;
  width: 100%;
  background: #F1F1F1;
}
.blank{
  height:100rpx;
  width: 100%;
}
/*举报*/
.feedback{
  margin: 40rpx;
  display: block;
  text-align: center;
  font-size: 30rpx;
  color: #5a5d86;
}