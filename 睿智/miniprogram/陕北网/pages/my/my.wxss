page{
  background: #F1F1F1;
}
.header{
  width: 700rpx;
  height:200rpx;
  background: #11D4C6;
  overflow: hidden;
  padding:100rpx 25rpx;
}
.titimg{
  width:150rpx;
  height:150rpx;
  border-radius: 50%;
  float: left;
}
.titname{
  width:470rpx;
  display: block;
  float: left;
  height:150rpx;
  margin-left: 30rpx;
  line-height:150rpx;
  font-size:42rpx;
  overflow: hidden;
  color: #FFF;
}


.centent{
  width: 650rpx;
  height: 100%;
  overflow: hidden;
  background: #FFF;
  margin:-80rpx 25rpx 50rpx 25rpx;
  padding:25rpx 25rpx 80rpx 25rpx;
}

.centent .cash{
 width:570rpx;
 height:120rpx;
 padding-left:80rpx;
 background: #FFF url(https://huahuohe.com/static/xcx/my1.png) no-repeat 15rpx center;
 background-size: 54rpx; 
 margin-bottom: 50rpx;
}
.cash .name{
  display: block;
  width:350rpx;
  height:60rpx;
  line-height:80rpx;
  font-size: 28rpx;
  color: #666;
  float: left;
}
.cash .checkcode{
  display: block;
  width:350rpx;
  height:60rpx;
  line-height:40rpx;
  font-size:38rpx;
  font-weight:bold;
    float: left;
}
.cash .btn{
  display: block;
  width: 200rpx;
  height:80rpx;
  font-size:30rpx;
  float:right;
  margin: 30rpx 0;
  background:#11D4C6; 
  border-radius: 25rpx;
  line-height: 80rpx;
  text-align: center;
  color: #FFF;
}
.centent navigator{
  display: block;
  width: 570rpx;
  height:100rpx;
  line-height:100rpx;
  font-size: 30rpx;
  padding-left:80rpx;
  border-bottom:2rpx solid #DDDDDD;
  color: #666666;
}
.centent .contact{
  display: block;
  width:650rpx;
  height:100rpx;
  line-height:100rpx;
  font-size: 30rpx;
  padding-left:80rpx;
  border-bottom:2rpx solid #DDDDDD;
  color: #666666;
  border-radius: 0;
  text-align: left;
}
.centent button::after{
  border: none;
}
.centent navigator:nth-child(2){
 background: url(https://huahuohe.com/static/xcx/my2.png) no-repeat 15rpx center,url(https://huahuohe.com/static/xcx/jiantouright.png) no-repeat right center;
  background-size:48rpx;
}
.centent button:nth-child(3){
  background: url(https://huahuohe.com/static/xcx/my3.png) no-repeat 15rpx center,url(https://huahuohe.com/static/xcx/jiantouright.png) no-repeat right center;
  background-size:48rpx;
}
.centent navigator:nth-child(4){
  background: url(https://huahuohe.com/static/xcx/my4.png) no-repeat 15rpx center,url(https://huahuohe.com/static/xcx/jiantouright.png) no-repeat right center;
  background-size:48rpx;
}
.centent navigator:nth-child(5){
  background: url(https://huahuohe.com/static/xcx/my5.png) no-repeat 15rpx center,url(https://huahuohe.com/static/xcx/jiantouright.png) no-repeat right center;
  background-size:48rpx;
}