.tabs{
    width: 720rpx;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    right: 0;
    padding: 0 15rpx;
    border-top: 1rpx solid #AAA;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    background: #F7F7FA;
    z-index: 999999;
}
.tabs navigator{
  width: 240rpx;
  height: 100rpx;
  text-align: center;
  line-height: 150rpx;
  font-size:26rpx;
  color:#11D4C6;
  display: block;
}
.tabs view{
  width: 240rpx;
  height: 100rpx;
  text-align: center;
  line-height: 150rpx;
  font-size:26rpx;
  color:#11D4C6;
  display: block;
}
.tabs .tabs_left{
  background-image:url(https://huahuohe.com/static/xcx/tab_fanhui.png);
  background-repeat: no-repeat;
  background-position:center 10rpx;
  background-size:46rpx;
}
.tabs .tabs_right{
  background-image:url(https://huahuohe.com/static/xcx/tab_baoming.png);
  background-repeat: no-repeat;
  background-position:center 10rpx;
  background-size:46rpx;
}
.tabs .tabs_center{
  position: relative;
  width: 150rpx;
  height:150rpx;
  top: -60rpx;
  left:0;
  border-radius: 50%;
  color: #FFF;
  line-height:225rpx;
  background-color:#FF6600;
  background-image:url(https://huahuohe.com/static/xcx/tab_zhichi.png);
  background-size:81rpx;
  background-repeat: no-repeat;
  background-position:center 10rpx;
  transform: scale(1);
  -webkit-transform: scale(1);
  -webkit-transition:ease all;
  -moz-transition:ease all;
  transition:ease all;
  -webkit-transition-duration:700ms;
  -moz-transition-duration:700ms;
  transition-duration:700ms;

}
