/*弹出选项卡*/
.codeshow{
  width: 750rpx;
  height:340rpx;
  overflow: hidden;
  background: #FFF;
  position:fixed;
  bottom: 0;
  left: 0;
  z-index: 9999999;
  animation:myfirst 0.3s linear;
}
.codeshow button,.codeshow navigator{
  width: 750rpx;
  height:107rpx;
  line-height:100rpx;
  text-align: center;
  font-size: 32rpx;
  border-bottom: 3rpx solid #AAA;
  background: #FFF;
  border-radius: 0rpx;
}
.codeshow button::after{border:none!important;}
.codeshow button:nth-last-child(1){
  border-top:10rpx solid #AAA;
  border-bottom: 0;
}

 @keyframes myfirst{
    0%{height: 0;}
    100%{height:340rpx;}
}

.text{
  display: block;
  line-height: 1.5;
  margin-bottom: 10rpx;
}