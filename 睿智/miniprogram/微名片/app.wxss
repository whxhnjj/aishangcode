/**app.wxss**/
 
page {
  box-sizing: border-box;
  background: #ffffff;
  font-size: 28rpx;
  color: rgb(51, 51, 51);
  font-family: PingFangSC-Regular, sans-serif, "微软雅黑";
}

.container {
  position: relative;
  width: 690rpx;
  margin: 0 auto;
}

.clearfix {
  display: table;
  content: '';
  clear: both;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

view, images, text, navigator {
  box-sizing: border-box;
}

button::after {
  border: none;
}

/*标题样式 */
.card_title{
  width: 100%;
  margin-bottom: 60rpx;
  text-align: center;
}
.card_title text:first-child{
  display: block;
  margin-bottom: 40rpx;
  color: rgb(51, 51, 51);
  font-weight: 600;
  font-size: 34rpx;
}
.card_title .card_title_line{
  display: block;
  width: 50rpx;
  height: 3rpx;
  margin: 0 auto;
  background: rgb(102, 102, 102);
}
.all_bottom{
  width: 750rpx;
  height: 50rpx;
  margin-bottom: 80rpx;
  background: #f5f5f5;
}

/* 兼容iponex */
.iponex_kuang{
  display: block;
  width: 100%;
  height: 68rpx;
}
.iponex {
  bottom: 68rpx !important;
}
.iponex_bottom{
  position: fixed;
  bottom: 0;
  right: 0;
  z-index: 100;
  width: 100%;
  height: 68rpx;
  background: #fff;
}

